<Window
    x:Class="WpfDependencyInjection.Views.ChildForm"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="clr-namespace:WpfDependencyInjection.ViewModels"
    xmlns:views="clr-namespace:WpfDependencyInjection.Views"
    Title="ChildForm"
    Width="300"
    Height="150"
    d:DataContext="{d:DesignInstance Type=viewmodels:ChildFormViewModel}"
    mc:Ignorable="d">

    <TextBlock
        HorizontalAlignment="Center"
        VerticalAlignment="Center"
        FontSize="20"
        Text="{Binding Message}" />

</Window>
